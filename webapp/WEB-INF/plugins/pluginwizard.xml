<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <name>pluginwizard</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>6.0.0-SNAPSHOT</version>
    <description>pluginwizard.plugin.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider>pluginwizard.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/pluginwizard/pluginwizard.png</icon-url>
    <copyright>Copyright 2001-2016 Mairie de Paris</copyright>
    <core-version-dependency>
		<min-core-version>4.1.0</min-core-version>
		<max-core-version/>	
	</core-version-dependency>
    <db-pool-required>1</db-pool-required>
     <!-- Admin Features -->
    <admin-features>
        <admin-feature>
            <feature-id>PLUGINWIZARD_MANAGEMENT</feature-id>
            <feature-title>pluginwizard.adminFeature.pluginwizard_management.name</feature-title>
            <feature-description>pluginwizard.adminFeature.pluginwizard_management.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/pluginwizard/ManageConfigurationKeys.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/pluginwizard/pluginwizard_feature.png</feature-icon-url>
        </admin-feature>
    </admin-features>
    
    <!-- Applications -->
    <applications>
        <application>
            <application-id>pluginwizard</application-id>
            <application-class>fr.paris.lutece.plugins.pluginwizard.web.PluginWizardApp</application-class>
        </application>
    </applications>

</plug-in>